    "version": "0.2.0",
    "configurations": [
        {
            "name": "DebugPETPLAYcpp",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/OVRINTERFACE/out/build/default/Debug/petplay.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "integratedTerminal",
            "preLaunchTask": "cmake-build-debug"
        },
        {
            "request": "launch",
            "name": "DEBUGFYOUNET1",
            "type": "node",
            "cwd": "${workspaceFolder}/FYOUNET",
            "console": "integratedTerminal",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--inspect=0.0.0.0:9228",
                "--allow-net",
                "--inspect-wait",
                "--allow-all",
                "main.ts",
                "client1",
                "8080",
                "127.0.0.1:8081"
            ],
            "attachSimplePort": 9228
        },
        {
            "request": "launch",
            "name": "DEBUGFYOUNET2",
            "type": "node",
            "cwd": "${workspaceFolder}/FYOUNET",
            "console": "integratedTerminal",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--allow-net",
                "--inspect-wait",
                "--allow-all",
                "main.ts",
                "client2",
                "8081",
                "127.0.0.1:8080"

            ],
            "attachSimplePort": 9229
        },
        
    ],
    "compounds": [
        {
            "name": "DebugAll",
            "configurations": [
                "DEBUGFYOUNET1",
                "DEBUGFYOUNET2"
            ]
        }
    ]
}
