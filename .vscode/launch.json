    "version": "0.2.0",
    "configurations": [
        {
            "name": "DebugPETPLAYcpp",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/OVRINTERFACE/out/build/default/Debug/petplay.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "integratedTerminal",
            "preLaunchTask": "cmake-build-debug"
        },
        {
            "request": "launch",
            "name": "DEBUGFYOUNET",
            "type": "node",
            "program": "${workspaceFolder}/FYOUNET/main.ts",
            "cwd": "${workspaceFolder}/FYOUNET",
            "console": "integratedTerminal",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--allow-net",
                "--inspect-wait",
                "--allow-all"
            ],
            "attachSimplePort": 9229
        }

    ]
}
